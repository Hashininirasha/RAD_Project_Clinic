            <Image url="@../images/nurse-removebg-preview.png" />
         </image>
      </ImageView>
      <Line endX="100.0" layoutY="159.0" startX="-100.0" stroke="WHITE" />
      <Button fx:id="btnTokenWindow" contentDisplay="RIGHT" graphicTextGap="2.0" layoutX="8.0" layoutY="173.0" mnemonicParsing="false" onAction="#onClickToken" prefHeight="67.0" prefWidth="183.0" styleClass="nav-btn" stylesheets="@../styles.css" text="Give Token" textAlignment="RIGHT">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="System Bold" size="9.0" />
         </font>
      </Button>
      <Button fx:id="btnAddPatient" layoutX="7.0" layoutY="250.0" mnemonicParsing="false" onAction="#onClickAddPatient" prefHeight="67.0" prefWidth="183.0" styleClass="nav-btn" stylesheets="@../styles.css" text="Add Patient" />
