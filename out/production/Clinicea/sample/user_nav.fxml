         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fitHeight="74.0" fitWidth="65.0" layoutX="2.0" layoutY="407.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/lock.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Button fx:id="btnClose" layoutX="750.0" layoutY="7.0" mnemonicParsing="false" onAction="#onCloseClick" style="-fx-background-color: #ffffff;" text="X">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <ImageView fitHeight="51.0" fitWidth="47.0" layoutX="11.0" layoutY="183.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/user.png" />
